@using LP.Plotter.Data;

<MudPaper>
    @foreach (var c in DataContext.SourceChannels)
    {
        <MudCheckBox @bind-Checked="@Label_CheckBox1" Label="@c.Name"></MudCheckBox>
    }
</MudPaper>

@code {
    [Parameter, EditorRequired]
    public VChannelSetVM DataContext { get; set; }

    public bool Label_CheckBox1 { get; set; } = true;
}
